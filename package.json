{
  "name": "@dip-in-milk/snake",
  "version": "1.0.1-2",
  "description": "Snake game engine",
  "main": "src/index.js",
  "scripts": {
    "start": "node --inspect ./node_modules/jest/bin/jest --watch --color --verbose=false",
    "lint": "tslint --project .",
    "prebuild": "rimraf build",
    "build": "tsc",
    "pretest": "npm run lint",
    "test": "jest --color"
  },
  "repository": {
    "type": "git",
    "url": "git+https://gitlab.com/dip-in-milk/snake.git"
  },
  "keywords": [
    "snake",
    "game"
  ],
  "author": "Stefan Teneff <st.teneff@dip-in-milk.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/dip-in-milk/snake/issues"
  },
  "homepage": "https://gitlab.com/dip-in-milk/snake#readme",
  "devDependencies": {
    "@babel/core": "^7.1.6",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.6",
    "@babel/runtime": "^7.1.5",
    "babel-core": "^7.0.0-bridge.0",
    "coveralls": "^3.0.2",
    "jest": "^23.6.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "^3.1.6"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "testEnvironment": "node",
    "testMatch": [
      "<rootDir>/test/!(mock|fixture)/**/*.js"
    ],
    "verbose": true
  },
  "dependencies": {}
}
